{
  "name": "i18node",
  "version": "0.5.0",
  "description": "I18n library for node with support for plurals and genders",
  "main": "index.js",
  "scripts": {
    "test": "mocha test",
    "lint": "eslint lib/i18node.js",
    "coverage": "istanbul cover --report lcov node_modules/mocha/bin/_mocha -- test/**/*.js",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "l10n"
  ],
  "author": {
    "name": "talyssonoc",
    "email": "talyssonoc@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/talyssonoc/i18node.git"
  },
  "bugs": {
    "url": "https://github.com/talyssonoc/i18node/issues"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "mustache": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^2.3.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.0.1",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3"
  }
}
